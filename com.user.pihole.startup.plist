<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.user.pihole.startup</string>
    <key>ProgramArguments</key>
    <array>
        <string>/bin/bash</string>
        <string>-c</string>
        <string>cd /Users/christianhendrick/pihole/pihole-config && /usr/local/bin/docker-compose up -d</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <false/>
    <key>StandardOutPath</key>
    <string>/tmp/pihole-startup.log</string>
    <key>StandardErrorPath</key>
    <string>/tmp/pihole-startup-error.log</string>
    <key>StartInterval</key>
    <integer>30</integer>
</dict>
</plist>
